void printPatt(int n)
{
   char ch='A';
    int i,j,k;
    for(i=n;i>=1;i--)
   {
       for(j=i;j<n;j++)
       {
           printf(" ");
       }
       for(k=1;k<=i;k++)
       { printf("%c",ch);
           ch++;
       }
       ch--;
       for(k=i+1;k<=2*i;k++)
       {
           printf("%c",ch);
           ch--;
       }ch++;
   printf("\n");
   }
}
